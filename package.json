{
  "name": "react-project",
  "description": "Fast and effective environment for frontend development",
  "author": "Maxim Lyulyaev",
  "version": "$(cat version.num)",
  "private": true,
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ],
  "workspaces": [
    "src/js/**/**"
  ],
  "scripts": {
    "start": "cd webpack && yarn start",
    "build": "cd webpack && yarn build",
    "server": "cd server && yarn start",
    "deploy": "cd server && now -d --public",
    "version-increment": "perl -i -pe 's/^((\\d+\\.)*)(\\d+)(.*)$/$1.($3+1).$4/e' < version.num version.num",
    "dockerbuild": "yarn version-increment && yarn build && sudo docker build -t lyulyaevma/react-project:$(cat version.num) . && sudo docker push lyulyaevma/react-project:$(cat version.num)",
    "precommit": "prettier --write '**/*.{js,jsx,json}'",
    "upgrade-packages": "yarn upgrade-interactive && cd webpack && yarn upgrade-interactive && cd ../server && yarn upgrade-interactive"
  },
  "dependencies": {
    "babel-plugin-dual-import": "^1.2.1",
    "babel-plugin-react-css-modules": "^3.4.2",
    "history": "^4.7.2",
    "loadable-components": "^2.2.2",
    "lodash-es": "^4.17.10",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.3.3",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "postcss-css-reset": "^1.0.2"
  },
  "optionalDependencies": {
    "babel-eslint": "^8.2.5",
    "eslint": "^5.0.1",
    "eslint-config-esnext": "^3.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "prettier": "^1.13.7"
  },
  "repository": "https://LyulyaevMaxim@github.com/LyulyaevMaxim/react-project.git",
  "license": "MIT"
}
