{
  "name": "react-project",
  "description": "Fast and effective environment for frontend development",
  "author": "Maxim Buslaev",
  "version": "$(cat version.num)",
  "scripts": {
    "dockerbuild": "yarn build && sudo docker build -t lyulyaevma/react-project:$(cat version.num) . && sudo docker push lyulyaevma/react-project:$(cat version.num)",
    "eslint-check": "eslint --print-config configs/.eslintrc.js | eslint-config-prettier-check",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "global": "^4.3.2",
    "history": "^4.7.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "superagent": "^3.8.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-polyfill": "^6.26.0",
    "eslint": "^4.16.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-flowtype": "^2.42.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^2.5.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "lint-staged": "^6.1.0",
    "lodash": "^4.17.4",
    "prettier": "^1.10.2",
    "prettierrc": "^0.0.0-5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "repository": "https://maxim-buslaev@github.com/maxim-buslaev/react-project.git",
  "license": "MIT"
}
