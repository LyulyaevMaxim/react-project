{
  "name": "react-project",
  "description": "Fast and effective environment for frontend development",
  "author": "Maxim Buslaev",
  "version": "$(cat version.num)",
  "scripts": {
    "github": "gh-pages -d dist",
    "version-increment": "perl -i -pe 's/^((\\d+\\.)*)(\\d+)(.*)$/$1.($3+1).$4/e' < version.num version.num",
    "dockerbuild": "yarn version-increment && yarn build && sudo docker build -t lyulyaevma/react-project:$(cat version.num) . && sudo docker push lyulyaevma/react-project:$(cat version.num)"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.40",
    "axios": "^0.18.0",
    "global": "^4.3.2",
    "history": "^4.7.2",
    "intl": "^1.2.5",
    "loadable-components": "^1.1.1",
    "normalizr": "^3.2.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0-beta.21",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "gh-pages": "^1.1.0",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "postcss-css-reset": "^1.0.2",
    "prop-types": "^15.6.0",
    "react-perf-devtool": "^3.0.2",
    "redux-logger": "^3.0.6"
  },
  "repository": "https://maxim-buslaev@github.com/maxim-buslaev/react-project.git",
  "license": "MIT"
}
