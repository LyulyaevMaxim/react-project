{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "standard",
    "plugin:react/recommended",
    "standard-react",
    "prettier-standard"
  ],
  "plugins": ["import", "react"],
  "rules": {}
}
